         version="3.1">
    <servlet>
        <servlet-name>select</servlet-name>
        <servlet-class>Servlets.DataBaseSelect.SelectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>select</servlet-name>
        <url-pattern>/select</url-pattern>
    </servlet-mapping>


